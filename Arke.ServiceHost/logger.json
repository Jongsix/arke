{
  "root_logger": "MyColoredConsoleLogger",
  "loggers": [
    {
      "logger_name": "MyColoredConsoleLogger",
      "logger_type": "PayTel.LogProvider.Factories.ColoredConsoleLoggerFactory",
      "layout": "${event-properties:item=Endpoint}|${event-properties:item=ChannelID}|${message}|${exception:format=tostring}",
      "stack_trace": {
        "error": false,
        "fatal": true
      },
      "level": {
        "min_level": "DEBUG",
        "max_level": "FATAL"
      },
      "console_color": {
        "INFO": "Green",
        "WARN": "Yellow",
        "DEBUG": "Cyan",
        "FATAL": "Red",
        "ERROR": "Magenta"
      },
      "swallowExceptions": "false"
    },
    {
      "logger_name": "MyGraylogEventInfoLogger",
      "logger_type": "PayTel.LogProvider.Factories.GraylogEventInfoLoggerFactory",
      "layout": "${message}|${onexception:${exception:format=tostring}",
      "level": {
        "min_level": "DEBUG",
        "max_level": "FATAL"
      },
      "stack_trace": {
        "error": false,
        "fatal": true
      },
      "includeSource": true,
      "hostName": "10.50.10.15:12201",
      "facility": "gelf",
      "includeStackTrace": true,
      "swallowExceptions": "false"
    },
    {
      "logger_name": "MyMultiplexingLogger",
      "logger_type": "PayTel.LogProvider.Factories.MultiplexingLoggerFactory",
      "loggers": [
        "MyColoredConsoleLogger",
        "MyGraylogEventInfoLogger"
      ],
      "swallowExceptions": "false"
    }
  ]
}
